
<style>
p.ex1 {
    padding: 10px 10px 10px 10px;
}
</style>


<div class="container">

<div class="row" >
  <div class="col-md-4"></div>
  <div class="col-md-4 text-center ">
  
  <p class="ex1">
    <img src="https://www.rustins.ltd/images/content/2_HomeLogo_237x93.png">
  </p>
  </div>

  <div class="col-md-4"></div>
</div>





<div class="row">
<div class="col-md-4"></div>

<div class="col-md-4">

<div class="panel panel-default">
  <div class="panel-heading"><h3>Log in to your account</h3></div>
  <div class="panel-body">



  <%= form_for @user_session do |f| %>
    <%= render 'shared/errors', object: @user_session %>
  
    <div class="form-group">
      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control' %>
    </div>
  
    <div class="form-group">
      <%= f.label :password %>
      <%= f.password_field :password, class: 'form-control' %>
    </div>
  

    <%= f.submit "Log in!", class: 'btn btn-lg btn-block btn-primary' %>
  <% end %>


  <div class="panel-footer"> 
   <%= link_to("New to Rustin's Sing up", users_new_path) %>
  </div>



    
  </div>
</div>

</div>

<div class="col-md-4"></div>
</div>


</div>


